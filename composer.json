{
    "name": "nacosvel/database-manager",
    "description": "Nacosvel Database Manager.",
    "keywords": [
        "Nacosvel Database Manager"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "nacosvel/container-interop": "^1.0",
        "nacosvel/database-manager-contracts": "^1.0",
        "nacosvel/facde": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nacosvel\\DatabaseManager\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "suggest": {
        "nacosvel/database-manager-implementation": "Required for full functionality. See available implementations: https://packagist.org/providers/nacosvel/database-manager-implementation"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
